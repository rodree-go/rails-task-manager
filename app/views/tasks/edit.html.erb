<h1>Edit Task</h1>

<%= form_with model: @task, url: update_task_path(@task), method: :patch do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed %>
  <%= f.check_box :completed %>
  <%= f.submit %>

<% end %>
